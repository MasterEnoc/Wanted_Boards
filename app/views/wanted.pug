doctype html
html
  head
    title Wanted Boards
    link(rel="stylesheet" type="text/css" href="./styles/ui.css")
  body
    nav(class="box-container")
      div(id="app-name") Wanted Boards
      div(id="board")= title
      div(id="username")= username

    div(id="boards-area")

      div(class="wanted-board" id="todo")
        h2 Todo

      div(class="wanted-board" id="doing")
        h2 Doing

      div(class="wanted-board" id="done")
        h2 Done
